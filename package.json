{
  "name": "couscous-restaurant",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run codegen:watch\" \"next dev\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "cypress:open": "cypress open",
    "generate": "graphql-codegen --config codegen.ts",
    "codegen:watch": "graphql-codegen --config codegen.ts --watch",
    "postinstall": "patch-package && npm run generate"
  },
  "dependencies": {
    "@graphql-typed-document-node/core": "^3.2.0",
    "@tanstack/react-query": "5.0.0",
    "@tanstack/react-query-devtools": "5.0.3",
    "@tanstack/react-query-next-experimental": "^5.0.0",
    "dotenv": "^16.3.1",
    "next": "13.5.4",
    "react": "^18",
    "react-dom": "^18",
    "react-glider": "^4.0.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/near-operation-file-preset": "3.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@graphql-codegen/typescript-react-query": "5.0.0",
    "@parcel/watcher": "^2.3.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "6.9.0",
    "concurrently": "^8.2.2",
    "cypress": "^13.3.0",
    "eslint": "^8",
    "eslint-config-next": "13.5.4",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-cypress": "^2.15.1",
    "graphql": "^16.8.1",
    "husky": "^8.0.0",
    "patch-package": "8.0.0",
    "sass": "^1.68.0",
    "typescript": "^5"
  }
}
